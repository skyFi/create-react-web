{
  "name": "create-react-web",
  "version": "0.3.2",
  "description": "easy to create react web project.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit-release:install": "yarn add standard-version standard-changelog commitizen cz-conventional-changelog -d",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix .",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "git:commit": "git add . && git cz",
    "release": "standard-version --commit-all",
    "git:push": "git push --follow-tags origin master",
    "onekey": "npm run git:commit && npm run git:push",
    "onekey:release": "npm run git:commit && npm run release && npm run git:push",
    "print-config": "node -p \"require('config')\"",
    "analyse": "gulp clean && gulp analyse",
    "dev": "gulp clean && gulp dev",
    "rebuild-icon": "gulp rebuild-icon"
  },
  "repository": "git+https://github.com/skyFi/create-react-web.git",
  "keywords": [
    "react",
    "web",
    "starter",
    "project"
  ],
  "author": "skylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skyFi/create-react-web/issues"
  },
  "homepage": "https://github.com/skyFi/create-react-web#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "cli-color": "^1.2.0",
    "commitizen": "^2.9.6",
    "commitlint": "^5.2.5",
    "compression": "^1.7.1",
    "config": "^1.28.1",
    "cookie-parser": "^1.4.3",
    "cssnano": "^3.10.0",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "ejs": "^2.5.7",
    "eslint-plugin-babel": "^4.1.2",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-changed": "^3.1.1",
    "gulp-file": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^0.4.6",
    "gulp-uglify": "^3.0.0",
    "happypack": "^4.0.0",
    "history": "^4.7.2",
    "hoist-non-react-statics": "^2.3.1",
    "husky": "^0.14.3",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "lodash": "^4.17.4",
    "merge-stream": "^1.0.1",
    "path-to-regexp": "^2.1.0",
    "postcss-loader": "^2.0.8",
    "promise-polyfill": "^6.0.2",
    "querystring": "^0.2.0",
    "rainbowlog": "^2.0.2",
    "raw-loader": "^0.5.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-mixin": "^4.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router4-redux": "^1.0.7",
    "react-web-helper": "0.0.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "standard-changelog": "^1.0.6",
    "standard-version": "^4.2.0",
    "style-loader": "^0.19.0",
    "svg-loader": "^0.0.2",
    "through-gulp": "^0.5.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "yaml-loader": "^0.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "standard-version": {
    "scripts": {
      "postbump": "git add ."
    }
  }
}
